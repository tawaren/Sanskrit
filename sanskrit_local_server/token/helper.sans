module Helper {
    public fun createIndex(consume d:$data1):(idx:$id) {
        idx = call $GenId[$data1](d);
        return idx;
    };

    public fun createRef(consume d:$data1):(ref:$ref) {
        idx = call $GenId[$data1](d);
        ref = call $ToRef[$id](idx);
        drop idx;
        return ref;
    };
}