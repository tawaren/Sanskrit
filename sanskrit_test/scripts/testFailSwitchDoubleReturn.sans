module TestFailSwitchDoubleReturn {
   type Option[T] {
       Some(T);
       None;
   };

   fun TestFunApply[<Drop, Copy, Value, Unbound> A](consume a:.Option[A], a2:A):(c:A, d:A) {
       (c,d) = switch a:.Option[A] {
          Some(a) => {
            b = copy a;
            return (a,b);
          };
          None => {
            b2 = a2;
            return (b2,b2);
          };
       };
       return (c,d);
   };
}