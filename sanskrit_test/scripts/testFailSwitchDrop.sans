module TestFailSwitchDrop {
   type Option[<Embed> T] {
       Some(T);
       None;
   };

   fun TestFunApply[<Embed, Drop> A](consume a:.Option[A], a2:A):() {
       b2 = a2;
       () = switch a:.Option[A] {
          Some(a) => {
            discard(a);
          };
          None => {
            discard(b2);
          };
       };
       return;
   };
}