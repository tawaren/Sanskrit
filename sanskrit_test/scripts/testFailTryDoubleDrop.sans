module TestFailTryDoubleDrop {
   err X;

   fun TestFunApply[<Copy, Drop> A](consume a:A):() {
       () = try {
          b = a;
          discard b;
          discard b;
          return;
       } catch {
         .X => {
           b = a;
           return;
         };
       };
       return;
   };
}