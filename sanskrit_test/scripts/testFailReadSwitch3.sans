module TestFailReadSwitch3 {

    public(consume, inspect) type X[A] {
        X(A);
        Y(A);
    };


    private fun Test[<Value, Unbound, Drop> A](a:.X[A], consume c:A):(b:A){
       b = inspect a:.X[A] {
            X(a) => {
                discard a;
                return c;
            };
            Y(a) => {
                discard a;
                return c;
            };
       };
       return b;
    };
}