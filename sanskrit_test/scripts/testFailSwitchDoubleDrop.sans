module TestFailSwitchDoubleDrop {
   type Option[<Embed> T] {
       Some(T);
       None;
   };

   fun TestFunApply[<Embed, Drop, Copy> A](consume a:.Option[A], a2:A):() {
       () = switch a:.Option[A] {
          Some(a) => {
            return;
          };
          None => {
            b2 = a2;
            discard b2;
            discard b2;
            return;
          };
       };
       return;
   };
}