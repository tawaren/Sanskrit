module IndexOps {
    public fun GenId[phantom T](consume in:System.Singleton[T]):(out:$id) {
        out = unpack in:System.Singleton[T];
        return out;
    };

    public fun ToRef(a:$id):(b:$ref) {
        b = call $ToRef[$id](a);
        return b;
    };

    public fun ToRef2(a:$data20):(b:$ref) {
        b = call $ToRef[$data20](a);
        return b;
    };

    public fun ToData(a:$ref):(c:$data20) {
        c = call $ToData[$ref,$data20](a);
        return c;
    };

    public fun ToData2(a:$id):(c:$data20) {
        c = call $ToData[$id,$data20](a);
        return c;
    };

    public fun DeriveIndex(a:$id, b:$id):(c:$id) {
        c = call $Derive[$id,$id](a,b);
        return c;
    };

    public fun DeriveRef(a:$ref, b:$ref):(c:$ref) {
        c = call $Derive[$ref,$ref](a,b);
        return c;
    };

    public fun ModIndex():(a:$id){
       a = moduleIndex;
       return a;
    };

    public fun ModIndex2():(b:$id){
       b = moduleIndex;
       return b;
    };
}