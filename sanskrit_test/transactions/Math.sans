module Math {

    public risky<Errors.Numeric> fun Add(a:IntU8.U8,b:IntU8.U8):(c:IntU8.U8) {
        c = call IntU8.add(a,b);
        return c;
    };

    public risky<Errors.Numeric> fun Add2(a:IntI128.I128,b:IntI128.I128):(c:IntI128.I128) {
        c = call IntI128.add(a,b);
        return c;
    };

    public fun ErrorAdd(a:IntU8.U8,b:IntU8.U8):(r:Bool.Bool) {
        r = try {
            c = call IntU8.add(a,b);
            r = pack Bool.Bool#False();
            return r;
        } catch {
            Errors.Numeric => {
                r = pack Bool.Bool#True();
                return r;
            };
        };
        return r;
    };

